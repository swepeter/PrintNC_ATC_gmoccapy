    ;g53 g1 Z #<z_pickup> F600
    ;G4 P0.5
    ;M65 P #<_ini[pins]spindle_release>
    ;M65 P #<_ini[pins]spindle_clean> 
    ;G4 P0.5

    g53 g0 Z #<_ini[rack_tc]z_safe>
